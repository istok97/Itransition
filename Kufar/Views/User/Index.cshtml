@using Microsoft.AspNetCore.Identity
@inject UserManager<User> UserManager
@model IEnumerable<Kufar.Models.User>
@{
    ViewData["Title"] = "Index";
}

<h2>User List</h2>
<head>
    <meta charset="utf-8"/>
</head>
<table class="table">
    <tr><th>Email</th><th>Year</th></tr>
    @foreach (var user in Model)
    {
        <tr >
        <td>@user.Email</td>
        <td>@user.Year</td>
        </tr>
    }
</table>
